@startuml
class 应用管理页面 {
  + 展示应用列表()
}
class 应用列表页面 {
  + 应用名称
  + 应用标识
  + 是否核心应用
  + Owner
  + 部署类型
  + 创建时间
  + 备注
}
class 创建应用页面 {
  + 选择部署类型()
  + 填写应用信息()
  + 提交信息()
}
class 填写应用信息页面 {
  + 应用名称
  + 应用标识
  + 应用描述
  + 负责人
  + 开发者
  + 程序语言
  + 是否核心应用
}
应用管理页面 "1" -- "1" 应用列表页面
应用管理页面 "1" -- "1" 创建应用页面
创建应用页面 "1" -- "1" 填写应用信息页面
@enduml